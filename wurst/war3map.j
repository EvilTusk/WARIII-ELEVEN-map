    call TriggerSleepAction(8.00)
    if ( ( udg_bPlayerDead[1] == true ) ) then
        call ResetToGameCameraForPlayer(Player(0), 2.00)
    else
        call DoNothing()
    endif
    if ( ( udg_bPlayerDead[2] == true ) ) then
        call ResetToGameCameraForPlayer(Player(1), 2.00)
    else
        call DoNothing()
    endif
    if ( ( udg_bPlayerDead[6] == true ) ) then
