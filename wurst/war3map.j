function Trig_Game_OverActions takes nothing returns nothing
    call DisplayTimedTextToForce(YDWEGetForceOfPlayerNull(GetOwningPlayer(GetDyingUnit())), 30, "TRIGSTR_103")
    call ResetToGameCameraForPlayer(GetOwningPlayer(GetDyingUnit()), 0)
    set udg_bPlayerDead[GetConvertedPlayerId(GetOwningPlayer(GetDyingUnit()))]=true
    call CameraSetupApplyForPlayer(true, gg_cam_Death_Cam, GetOwningPlayer(GetDyingUnit()), 0.00)
