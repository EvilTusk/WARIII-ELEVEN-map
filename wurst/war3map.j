    call ForceAddPlayerSimple(Player(1), udg_Playersgroup)
    call ForceAddPlayerSimple(Player(5), udg_Playersgroup)
    call ForceAddPlayerSimple(Player(6), udg_Playersgroup)
    call ForForce(udg_Playersgroup, function Trig_Initialize_WarChasersFunc012002)
    set bj_forLoopAIndex=0
    set bj_forLoopAIndexEnd=12
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_bPlayerDead[GetForLoopIndexA()]=false
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    set bj_forLoopAIndex=0
    set bj_forLoopAIndexEnd=12
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_bDoors[GetForLoopIndexA()]=false
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call SetCameraTargetControllerNoZForPlayer(Player(0), gg_unit_ewsp_0006, 0, 0, false)
    call SetCameraTargetControllerNoZForPlayer(Player(1), gg_unit_ewsp_0007, 0, 0, false)
    call SetCameraTargetControllerNoZForPlayer(Player(5), gg_unit_ewsp_0008, 0, 0, false)
    call SetCameraTargetControllerNoZForPlayer(Player(6), gg_unit_ewsp_0010, 0, 0, false)
    // If a player is playing intialize the variable to that player's wisp, else remove that wisp
    if ( ( GetPlayerSlotState(Player(0)) == PLAYER_SLOT_STATE_PLAYING ) ) then
        set udg_Player1=gg_unit_ewsp_0006
    else
        call RemoveUnit(gg_unit_ewsp_0006)
    endif
    if ( ( GetPlayerSlotState(Player(1)) == PLAYER_SLOT_STATE_PLAYING ) ) then
        set udg_Player2=gg_unit_ewsp_0007
