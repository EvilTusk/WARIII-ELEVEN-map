// Trigger: Side Fire Trap 3 Damage
//===========================================================================
function Trig_Side_Fire_Trap_3_DamageFunc004002 takes nothing returns nothing
    call SetUnitLifeBJ(GetEnumUnit(), ( GetUnitStateSwap(UNIT_STATE_LIFE, GetEnumUnit()) - 30.00 ))
endfunction
function Trig_Side_Fire_Trap_3_DamageActions takes nothing returns nothing
    call GroupClear(udg_FlameDamage3)
    call GroupAddGroup((YDWEGetUnitsInRectMatchingNull((gg_rct_Flame_Trap_4) , null)), udg_FlameDamage3) // INLINED!!
    if ( ( IsUnitGroupEmptyBJ(udg_FlameDamage3) == true ) ) then
        return
    else
        call DoNothing()
    endif
    call ForGroupBJ(udg_FlameDamage3, function Trig_Side_Fire_Trap_3_DamageFunc004002)
    call AddSpecialEffectTargetUnitBJ("origin", GroupPickRandomUnit(udg_FlameDamage3), "Abilities\\Weapons\\RedDragonBreath\\RedDragonMissile.mdl")
    call DestroyEffect(GetLastCreatedEffectBJ())
endfunction
