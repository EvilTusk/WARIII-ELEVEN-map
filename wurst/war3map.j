        if ( ( ( IsUnitAliveBJ(udg_MonsterSpawners[GetForLoopIndexA()]) == true ) and ( CountUnitsInGroup(udg_MonsterGroups[GetForLoopIndexA()]) < udg_MaxMonsters[GetForLoopIndexA()] ) ) ) then
            call TriggerExecute(gg_trg_Spawn_One_Monster)
        else
            call DoNothing()
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
endfunction
