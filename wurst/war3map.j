function Trig_Initialize_WarChasersActions takes nothing returns nothing
    call Preloader("scripts\\WarChasers.pld")
    // Nick Lawson: All initially visible stuff should be done before the first 'wait'
    call SetTimeOfDay(24.00)
    call UseTimeOfDayBJ(false)
    call FogMaskEnableOn()
    call SetTerrainFogExBJ(0, 1400.00, 5000.00, 0, 10.00, 30.00, 50.00)
