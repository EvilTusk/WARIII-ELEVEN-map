//===========================================================================
// Trigger: Spawn One Monster
//===========================================================================
function Trig_Spawn_One_MonsterActions takes nothing returns nothing
    // Addition for turning them on and off
    if ( ( udg_EnableSpawners[GetForLoopIndexA()] == false ) ) then
        return
    else
        call DoNothing()
    endif
    // I spawn one monster every time 'frequency' gets to zero
    set udg_Frequencies[GetForLoopIndexA()]=( udg_Frequencies[GetForLoopIndexA()] - 1 )
    if ( ( udg_Frequencies[GetForLoopIndexA()] != 0 ) ) then
        return
    else
        call DoNothing()
    endif
    // I restore the frequency
    set udg_Frequencies[GetForLoopIndexA()]=udg_InitialFrequencies[GetForLoopIndexA()]
    call CreateNUnitsAtLoc(1, udg_SpawnTypes[GetForLoopIndexA()], Player(11), GetUnitLoc(udg_MonsterSpawners[GetForLoopIndexA()]), GetRandomReal(0, 360.00))
    call GroupAddGroup(GetLastCreatedGroup(), udg_MonsterGroups[GetForLoopIndexA()])
    // We could even play a special effect, here.
    call AddSpecialEffectTargetUnitBJ("origin", GetLastCreatedUnit(), "Abilities\\Spells\\Undead\\AnimateDead\\AnimateDeadTarget.mdl")
endfunction
